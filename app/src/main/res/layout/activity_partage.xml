<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nowastit.nowastit_v3_.PartageActivity"
    android:background="@drawable/inscription">

    <Button
        android:id="@+id/PartageBt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="Proposer un produit"
        />

    <Button
        android:id="@+id/VoirCarteBt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voir les produits"
        android:layout_marginTop="67dp"
        android:layout_below="@+id/PartageBt"
        android:layout_centerHorizontal="true"
        />

</RelativeLayout>
